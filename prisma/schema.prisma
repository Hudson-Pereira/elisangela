generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model produtos {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  nome      String
  valor     String
  descricao String
  dia       String
  mes       String
  ano       String
  estoque   String
  vendedor  String
}
